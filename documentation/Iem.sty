% Use the following packages

\usepackage[T1]{fontenc}
\usepackage[latin1]{inputenc}
%\usepackage{t1enc}
\usepackage{latexsym}
\usepackage{color}
\usepackage{sectsty}
\definecolor{iemcolor}{rgb}{0.16, 0.26, 0.58}
% Font selection
%\renewcommand{\familydefault}{cmss}
\renewcommand{\familydefault}{ptm}
\allsectionsfont{\normalfont\sffamily\bfseries} 

% Graphics package graphics
% usage : \includegraphics{figure.ps} within figure environment
\usepackage{epsfig,graphics}  

% Graphics package subfigure 
% using multiple graphics in one figure environment 
% usage : \subfigure[..]{\includegraphics{filename}} 
%\usepackage{subfigure}  

% Mathematics
\usepackage{amsmath, amssymb}

% URL formatting package url
% typesets Url in teletype and manages splitting over lines
% usage : \url{http://www.example.com}
\usepackage{url}

% Page dimensions 
% either in inches or in cm
\setlength{\textwidth}{15cm}
\setlength{\textheight}{23cm}
\setlength{\topmargin}{0cm}
\setlength{\headheight}{0.5cm}
\setlength{\headsep}{0.5cm}
\setlength{\oddsidemargin}{1cm}
\setlength{\evensidemargin}{1cm}

% Paragraph style 
\setlength{\parindent}{0pt}
\setlength{\parskip}{1ex}

% Specifying the page-style by 
% \markright{} : set the right-heading
% \pagenumbering{roman} : i,ii,iii,iv ... numbering 
\pagestyle{myheadings}
\markright{}
\pagenumbering{roman}

% use wide spacing (1.5-2)
% larger typeset
% 12pt typeset is normal-size as specified in \documentclass 
%\renewcommand{\baselinestretch}{1.66}
%\normalsize

% Doctype could be ``Thesis'', ``Essay'' etc.
\newcommand{\doctype}[1]{\gdef\@doctype{#1}}
\doctype{}

\newcommand{\supervisor}[1]{\gdef\@supervisor{#1}}
\supervisor{}
\newcommand{\institute}[1]{\gdef\@institute{#1}}
\institute{}


